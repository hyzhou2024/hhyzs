/* pages/jx/jx.wxss */
page{
    background-color: #074199;
    
  }
  .container{
    width: 96%;
    margin: 0 auto;
    color: rgb(248, 246, 246);
    font-size: 30rpx;
  }
  .ssq_container_red{
    display: flex;
    width: 100%;
    margin: 10rpx auto;
    flex-wrap: wrap;
    background-color: rgb(107, 104, 104);
    border-radius: 20rpx;
    
  }
  .ssq_container_blue{
    display: flex;
    width: 100%;
    margin: 10rpx auto;
    flex-wrap: wrap;
    background-color: rgb(80, 80, 83);
    border-radius: 20rpx;
    
  }
  .ssq_num_red{
    display: flex;
    margin: 10rpx;
    color: rgb(248, 246, 246);
    width: 70rpx;
    height: 70rpx;
    background-color: rgb(145, 141, 141);
    align-items: center;
    justify-content: center;
    border-radius: 50%;
  }
  .ssq_num_blue{
    display: flex;
    margin: 10rpx;
    color: rgb(248, 246, 246);
    width: 70rpx;
    height: 70rpx;
    background-color: rgb(102, 104, 105);
    align-items: center;
    justify-content: center;
    border-radius: 50%;
  }
  .ssq_num_select{
    display: flex;
    margin: 10rpx;
    color: rgb(248, 246, 246);
    width: 75rpx;
    height: 75rpx;
    background-color: rgb(102, 104, 105);
    align-items: center;
    justify-content: center;
    border-radius: 50%;
  }
  .ssq_num_red_select{
    background-color: #f00;
  }
  .btn{
      display: flex;
      width: 100%;
  }
  .button_view{
    width: 50%;
    margin: 0rpx auto;
    background-color: rgb(102, 104, 105);
    color: rgb(248, 246, 246);
    font-size: 32rpx;
  }
  .button_view_1{
    width: 50%;
    margin: 0rpx auto;
    background-color: rgb(87, 122, 139);
    color: rgb(248, 246, 246);
    font-size: 32rpx;
  }
  
  .ssq_num_blue_select{
    background-color: rgb(0, 140, 255);
  }