<!--component/ssq/ssq.wxml-->


<view class='container1' wx:if="{{config.showLottery}}">
  <!-- 开奖号码 -->
  <view class="open-number-box">
    <view  wx:if="{{ssq.detail.OpenNumber}}">
      <text>第<text id='newLastOpenIssueName'>{{ssq.detail.Name}}</text>期开奖号码</text>
      <view class='open-number'>
        <block wx:for="{{ssq.detail.OpenNumber.hq}}" wx:for-item="Num" wx:key="hq">
          <text>{{Num}}</text>
        </block>
        <block wx:for="{{ssq.detail.OpenNumber.lq}}" wx:for-item="Num" wx:key="lq">
          <text class='blue-ball'>{{Num}}</text>
        </block>
       <navigator url='../lotterylist/lotterylist?lotId=5&lotName=双色球往期&typeId=1' class='moveinfo'>
          <image src='../../images/youjiantou.png'></image>
        </navigator>
      </view>
    </view>
    <view wx:else>
      <text>第<text id='newLastOpenIssueName'>{{ssq.detail.Name}}</text>期正在开奖</text>
      <view class='open-number'>
        <text>?</text>
        <text>?</text>
        <text>?</text>
        <text>?</text>
        <text>?</text>
        <text>?</text>
        <text class='blue-ball'>?</text>
      </view>
    </view>
  
  </view>
  <!-- 销售信息 -->
  <view class="sale-info-box sale-info-item" style="height:130rpx;">
        <view style="height:130rpx;">
            <text style="display:block;height:65rpx;line-height:65rpx;color:#666;  border-top: 1rpx solid #152e52;">全国销量</text>
            <text style="display:block;height:65rpx;line-height:65rpx;color:#ff6a6e;">{{ssq.detail.Sales}}元</text>
        </view>
        <view style="height:130rpx;">
            <text style="display:block;height:65rpx;line-height:65rpx;color:#666;  border-top: 1rpx solid #152e52;">全国奖池</text>
            <text style="display:block;height:65rpx;line-height:65rpx;color:#ff6a6e;">{{ssq.detail.PoolMoney}}元</text>
        </view>
    </view>

   
</view>




