
.open_container {
    align-items: center;
    color: #666;
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    margin: 0 auto;
    position: relative;
    width: 92%;
}

.open_container image {
    height: 80rpx;
    position: relative;
    width: 80rpx;
}


.open_openbox {
    background-color: #fff;
    border-radius: 12rpx;
    margin: 12rpx auto;
    width: 96%;
}

.openlist_number {
    border-radius: 14rpx;
    padding-bottom: 40rpx;
    text-align: center;
    width: 100%;
}

.openlist_number .next {
    height: 60rpx;
    position: absolute;
    width: 60rpx;
}

.openlist_number .title {
    color: #333;
    font-size: 26rpx;
}

.open-number-fc {
    color: #fff;
    /* border: 1rpx solid #e60013; */
}


.open-number-fc text {
    background-color: #f72b2b;
    border-radius: 50%;
    display: inline-block;
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-right: 10rpx;
    text-align: center;
    width: 50rpx;
}
.open-number-tc text {
    background-color: #f72b2b;
    border-radius: 50%;
    display: inline-block;
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-right: 20rpx;
    margin-top: 10rpx;
    text-align: center;
    width: 50rpx;
}
.open-number-fc .blue-ball {
    background-color: #2cadf8;
}
.open-number-tc .dlt_q {
    background-color: #49c4f5;
}
.open-number-tc .dlt_h {
    background-color: #fdf911;
    color: #000;
}
.open-number-tc .qxc_q {
    background-color: #4b54a6;
}

.open-number-tc .qxc_h {
    background-color: #e9ab50;
}

.open-number-tc .pl {
    background-color: #c66fb2;
}

.open-bottom {
    display: flex;
}

.open-play {
    color: #136bd7;
    padding-left: 24rpx;
    text-align: left;
}

.open-play,.open-time {
    flex: 1;
    font-size: 28rpx;
}

.open-time {
    color: #333;
    display: flex;
    padding-right: 25rpx;
    text-align: right;
}

.open-time .time {
    flex: 2.5;
    padding: 5rpx 10rpx 2rpx;
}

.form_box {
    text-align: center;
}

.form_box,.form_box .btn {
    flex: 0.5;
}

.form_box .btn {
    background-color: #3d8bff;
    border: none;
    height: 44rpx;
    line-height: 44rpx;
    margin: 0;
    padding: 0;
}

.form_box .txt {
    color: #fff;
    font-size: 28rpx;
    height: 44rpx;
    width: 100%;
}

button::after {
    border: none;
}

.form_box .active {
    background-color: #acabb0;
}






.sale-info-box {
    align-items: center;
    background-color: #fff;
    border-bottom: 1rpx solid #f2f2f2;
    color: #666;
    display: flex;
    height: 100rpx;
    justify-content: space-around;
    text-align: center;
    width: 100%;
}

.sale-info-box view {
    border-right: 1rpx solid #f2f2f2;
    flex: 1;
    line-height: 100rpx;
}

.sale-info-box view:last-child {
    border-right: 0;
}

.sale-info-box:last-child {
    border-bottom: 0;
}

.share_btn {
    background-color: #fb4b3f;
    border-radius: 100rpx;
    color: #fff;
    font-size: 32rpx;
    height: 80rpx;
    width: 480rpx;
}

.content_team {
    background-color: #fff;
    border-top: 1rpx solid #f2f2f2;
    padding-top: 34rpx;
    width: 100%;
}

.content_team .box {
    display: flex;
    flex-wrap: wrap;
    margin: 0 auto;
    text-align: center;
    width: 96%;
}

.content_team .item {
    height: 180rpx;
    margin-bottom: 34rpx;
    width: 20%;
}

.content_team .insetBox {
    height: 154rpx;
    margin-left: 10%;
    position: relative;
    width: 80%;
}

.content_team .item image {
    height: 10rpx;
    width: 10rpx;
}

.content_team .item .title {
    bottom: 0;
    color: #333;
    font-size: 28rpx;
    overflow: hidden;
    position: absolute;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
}

.copyBox {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}

.copy_btn {
    background: rgba(0,0,0,.8);
    border: 1rpx solid #fff;
    border-radius: 5rpx;
    color: #fff;
    font-size: 32rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-left: 30rpx;
    padding: 6rpx 10rpx;
    width: 100rpx;
}

.swiper_message {
    background-color: #fff;
    border-radius: 12rpx;
    height: 100rpx;
    margin: 24rpx auto 0;
    width: calc(100% - 48rpx);
}

.swiper_item {
    align-items: center;
    display: flex;
    font-size: 32rpx;
    height: 100rpx;
    justify-content: center;
    text-align: center;
    width: 100%;
}

.swiper_item .swiper_content {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    display: -webkit-box;
    flex: 4;
    overflow: hidden;
}

.swiper_item .copy_btn {
    flex: 1;
}

.copy_message {
    background: rgba(0,0,0,.8);
    border: 1rpx solid #fff;
    border-radius: 5rpx;
    color: #fff;
    padding: 5rpx 10rpx;
}

#set_xcx_pop {
    background: rgba(0,0,0,.8);
    filter: alpha(opacity=80);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 100;
}

#set_xcx_pop .txt {
    color: #fff;
    font: 700 36rpx/40rpx simsun;
    left: 5%;
    position: absolute;
    text-shadow: 0 1rpx 0 #ddd;
    top: 5%;
}

#set_xcx_pop .txt:after {
    content: "â†‘";
    display: inline-block;
    font-size: 2.5rem;
    margin-left: 1rem;
    position: relative;
    top: 0;
    transform: rotate(15deg);
}

