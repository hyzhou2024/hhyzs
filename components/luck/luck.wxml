<view class="money_container" style="background:{{newColor}}" wx:if="{{config.showLottery}}">
  <view class="h1">
    <view>红包券</view>
    <view>红包规则？</view>
  </view>
  <view class="h2">
    <view>{{userProfile.jifen}}</view>
    <navigator class="tx" url="/pages/hb/tx">
      <view class="tx_btn">立即提现</view>
    </navigator>
  </view>
  <view class="h3">
    <view>满0.3元可提现，秒到微信零钱</view>
    <navigator url="/pages/hb/hb">红包券明细</navigator>
  </view>

</view>
<view class="msg_container" wx:if="{{config.showLottery}}">
  <image src="../../images/tongzhi.png" mode="widthFix"></image>
  <view class="text">中奖啦</view>
  <swiper class="swiper_container " vertical="true" autoplay="true" circular="true" interval="3000">
    <block>
      <view url="{{item.url}}" wx:for="{{msg_list}}" wx:for-item="item" wx:key="id">
        <swiper-item bindtap="bindJoin">
          <view class="list_des" style="color:#3b90f1">
            <view class="list_title">
              <view>恭喜</view>
              <image src="{{item.avatar_url}}" mode="widthFix"></image>
              <view style="color: #e60013;">{{item.nickname}}</view>
              <view>参与幸运抽奖获得</view>
              <view style="color: #e60013;">{{item.final_gain}}</view>
              <view>红包券</view>
            </view>
          </view>
        </swiper-item>

      </view>
    </block>
  </swiper>
  <!-- <view class="join_a" bindtap="bindJoin">立即参与</view> -->

</view>
<view class="tabbar"wx:if="{{config.showLottery}}">
  <scroll-view scrollX="true" class="flex_row prize_type_container" scroll-into-view="{{name}}">
    <view bindtap="selectPrize" class="flex_column prize_type" data-name="{{luckType.name}}" wx:for="{{luckTypeList}}" wx:for-item="luckType" wx:key="key" id="{{luckType.name}}">
      <view class="{{luckType.name===name?'title_selected':'title_unselected'}}">{{luckType.desc}}</view>
      <image mode="aspectFit" src="https://resource.cgt-cwl.com.cn/image/2021-08-10-arrow_entity_up.png" style="width: 20rpx; height: 20rpx;" wx:if="{{luckType.name===name}}"></image>
    </view>
  </scroll-view>
</view>
<view class="body" wx:if="{{config.showLottery&&name == 'lucklist'}}">
  <view class="body-content" wx:if="{{config.showLottery}}">
    <view class="body-item">
      <view class="b-item" bindtap="jump2detial" data-id="lucklist" data-data="{{item.issue}}" wx:for="{{luckList}}" wx:key="index">
        <view class="item-image">
          <image mode="heightFix" src="https://minidataservice.com/images/hongbao/main.jpg"></image>
        </view>
        <view class="item-txt">
          <view class="txt-title" style="color:{{item.isopen==true?'':newColor}};">{{item.description}}</view>
          <view class="txt-title-rule end-activity" wx:if="{{config.showLottery}}">
            <view>{{item.issue}}期</view>
            <view class="end-activity-txt txt-blue" style="color:{{item.isopen==true?'':newColor}};">{{item.isopen==true?'【活动已结束】':'【New】'}}</view>
          </view>
          <view class="txt-title-rule txt-title-color">
            <view class="txt-p">
              <view>奖池:</view>
              <view class="hiegh-color">{{item.jiangchi}}</view>
              <view>红包券</view>
            </view>
          </view>
          <view class="txt-title-rule txt-title-color-blue">

            <view class="txt-p">
              <view>人数:</view>
              <view class="hiegh-color">{{item.joinnum}}</view>
              <view>人</view>
            </view>

          </view>
          <view class="txt-title-rule end-activity" style="margin-top: 5rpx;">
            <view class="txt-p">
              <view wx:if="{{item.isopen}}">【{{item.opendate}}】已开奖</view>
              <view wx:else style="display: flex; color:{{newColor}}">距开奖时间还有：<view style="color:{{item.isopen==false?'':newColor}}">
                  <countdown lasttime='{{item.opendate}}'></countdown>
                </view>
              </view>
            </view>

          </view>
        </view>

      </view>
    </view>
  </view>
</view>

<view class="body" wx:if="{{config.showLottery&&name == 'ssqlist'}}">
  <view class="body-content" wx:if="{{config.showLottery}}">
    <view class="body-item">
      <view class="b-item" bindtap="jump2detial" data-id="ssqlist" data-data="{{item.code}}" wx:for="{{ssqList}}" wx:key="index">
        <view class="item-image" style="background-color: rgb(252, 216, 212);">
          <view>第{{item.code}}期</view>
          <view class="blueball" wx:if="{{item.blue== null}}"><text>?</text></view>
          <view class="blueball" wx:else><text>{{item.blue}}</text></view>
        </view>
        <view class="item-txt">
          <view class="txt-title-rule end-activity" wx:if="{{config.showLottery}}">
            <view class="txt-title" style="color:{{item.isopen==true?'':newColor}};">{{item.name}}</view>
            <view>{{item.code}}期</view>
            <view class="end-activity-txt txt-blue" style="color:{{item.isopen==true?'':newColor}};">{{item.isopen==true?'【活动已结束】':'【New】'}}</view>
          </view>
          <view class="txt-title-rule txt-title-color">
            <view class="txt-p">
              <view>奖池:</view>
              <view class="hiegh-color">{{item.prizepool}}</view>
              <view>红包券</view>
            </view>


          </view>
          <view class="txt-title-rule txt-title-color-blue">

            <view class="txt-p">
              <view>人数:</view>
              <view class="hiegh-color">{{item.joinnum}}</view>
              <view>人</view>
            </view>
            <view class="txt-p">
              <view wx:if="{{!item.status}}">>>立即参与</view>
            </view>

          </view>
          <view class="txt-title-rule end-activity" style="margin-top: 5rpx;">
            <view class="txt-p" wx:if="{{name == 'lucklist'}}">
              <view wx:if="{{item.isopen}}">【{{item.opendate}}】已开奖</view>
              <view wx:else style="display: flex; color:{{newColor}}">距开奖时间还有：<view style="color:{{item.isopen==false?'':newColor}}">
                  <countdown lasttime='{{item.opentime}}'></countdown>
                </view>
              </view>
            </view>
            <view class="txt-p" wx:if="{{name == 'ssqlist'}}">
              <view wx:if="{{item.status}}">【{{item.code}}】已开奖</view>
              <view wx:else style="display: flex; color:{{newColor}}">距开奖时间还有：<view style="color:{{item.status==false?'':newColor}}">
                  <countdown lasttime='{{item.opentime}}'></countdown>
                </view>
              </view>
            </view>

          </view>

        </view>

      </view>
    </view>
  </view>
</view>